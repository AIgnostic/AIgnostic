{
    "name": "report-gen",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "packages/report-gen/report-gen",
    "targets": {
      "lock": {
        "executor": "@nxlv/python:run-commands",
        "options": {
          "command": "poetry lock --no-update",
          "cwd": "packages/report-gen"
        }
      },
      "add": {
        "executor": "@nxlv/python:add",
        "options": {}
      },
      "update": {
        "executor": "@nxlv/python:update",
        "options": {}
      },
      "remove": {
        "executor": "@nxlv/python:remove",
        "options": {}
      },
      "build": {
        "executor": "@nxlv/python:build",
        "outputs": [
          "{projectRoot}/dist"
        ],
        "options": {
          "outputPath": "packages/report-gen/dist",
          "publish": false,
          "lockedVersions": true,
          "bundleLocalDependencies": true
        },
        "cache": true
      },
      "install": {
        "executor": "@nxlv/python:install",
        "options": {
          "silent": false,
          "args": "",
          "cacheDir": ".cache/pypoetry",
          "verbose": false,
          "debug": false
        }
      },
      "lint": {
        "executor": "@nxlv/python:flake8",
        "outputs": [
          "{workspaceRoot}/reports/report-gen/pylint.txt"
        ],
        "options": {
          "outputFile": "reports/report-gen/pylint.txt"
        },
        "cache": true
      },
      "test": {
        "executor": "@nxlv/python:run-commands",
        "outputs": [
          "{workspaceRoot}/reports/report-gen/unittests",
          "{workspaceRoot}/coverage/report-gen"
        ],
        "options": {
          "command": "poetry run pytest tests/",
          "cwd": "packages/report-gen"
        },
        "cache": true
      }
    },
    "tags": [],
    "release": {
      "version": {
        "generator": "@nxlv/python:release-version"
      }
    }
  }
  